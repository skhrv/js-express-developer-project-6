{
   "name": "fastify-nodejs-application",
   "version": "0.1.0",
   "type": "module",
   "description": "fastify-nodejs-application",
   "engines": {
      "node": ">=16.x"
   },
   "scripts": {
      "prebuild": "rm -rf dist",
      "test": "jest --verbose --runInBand",
      "build": "webpack",
      "lint": "npx rome check . --apply-suggested",
      "format": "npx rome format . --write",
      "start": "fastify start server/plugin.js -l debug -P -o",
      "prepare": "husky install"
   },
   "jest": {
      "modulePathIgnorePatterns": [
         "<rootDir>/__tests__/helpers/"
      ],
      "setupFiles": [
         "dotenv/config"
      ]
   },
   "author": "Stanislav Dzisiak",
   "license": "ISC",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/hexlet-boilerplates/fastify-nodejs-application.git"
   },
   "bugs": {
      "url": "https://github.com/hexlet-boilerplates/fastify-nodejs-application/issues"
   },
   "homepage": "https://github.com/hexlet-boilerplates/fastify-nodejs-application.git#readme",
   "dependencies": {
      "@fastify/formbody": "7.0.1",
      "@fastify/passport": "2.2.0",
      "@fastify/secure-session": "^6.0.0",
      "@fastify/sensible": "5.1.0",
      "@fastify/static": "6.5.0",
      "@fastify/view": "7.1.0",
      "@vscode/sqlite3": "5.0.8",
      "bootstrap": "5.2.0",
      "fastify": "4.10.2",
      "fastify-cli": "5.1.0",
      "fastify-error-page": "4.0.0",
      "fastify-method-override": "1.5.9",
      "fastify-objectionjs": "2.0.0",
      "fastify-reverse-routes": "3.0.0",
      "husky": "^8.0.3",
      "i18next": "21.9.1",
      "jquery": "3.6.0",
      "knex": "2.4.0",
      "lodash": "4.17.21",
      "mini-css-extract-plugin": "2.6.1",
      "npm-check-updates": "16.0.5",
      "objection": "3.0.1",
      "objection-unique": "1.2.2",
      "pg": "^8.9.0",
      "pino-pretty": "9.0.1",
      "pug": "3.0.2",
      "qs": "6.11.0",
      "sqlite3": "^5.1.6",
      "webpack": "^5.79.0"
   },
   "devDependencies": {
      "@jest/globals": "28.1.3",
      "@types/jest": "28.1.7",
      "@types/node": "18.7.11",
      "css-loader": "6.7.1",
      "dotenv": "16.0.1",
      "eslint": "8.22.0",
      "eslint-config-airbnb-base": "15.0.0",
      "eslint-plugin-import": "2.26.0",
      "eslint-plugin-jest": "26.8.7",
      "jest": "28.1.3",
      "jest-cli": "28.1.3",
      "pino-pretty": "9.0.1",
      "rome": "^11.0.0",
      "style-loader": "3.3.1",
      "webpack-cli": "4.10.0"
   }
}
